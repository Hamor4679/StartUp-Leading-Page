{
  "name": "gulp_project",
  "version": "1.0.0",
  "description": "my second build of gulp",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "path": {
    "build": {
      "html": "dist/",
      "css": "dist/css/",
      "js": "dist/js/",
      "img": "dist/img/",
      "fonts": "dist/fonts/",
      "icon": "dist/icon/",
      "lib": "dist/libs/"
    },
    "src": {
      "html": "src/*.html",
      "sass": "src/sass/main.scss",
      "js": "src/js/main.js",
      "img": "src/img/**/*.{jpg,png,svg,gif,ico,webp}",
      "fonts": "src/fonts/**/*.ttf",
      "icon": "src/icon/**/*.{eot,svg,ttf,woff,woff2}"
    },
    "lib": {
      "js": "src/libs/libs.js",
      "css": "src/libs/libs.css"
    },
    "watch": {
      "html": "src/**/*.html",
      "sass": "src/sass/**/*.scss",
      "js": "src/js/**/*.js",
      "img": "src/img/**/*.{jpg,png,svg,gif,ico,webp}",
      "fonts": "src/fonts/**/*.ttf",
      "icon": "src/icon/**/*.{eot,svg,ttf,woff}",
      "libs": "src/libs/"
    },
    "clean": "./dist/"
  },
  "author": "stopinov roman",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-cssimport": "^7.0.0",
    "gulp-file-include": "^2.2.2",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-include": "^2.4.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-shorthand": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-ttf2woff": "^1.1.1",
    "gulp-ttf2woff2": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "node-bourbon": "^4.2.8",
    "normalize.css": "^8.0.1",
    "rfs": "^9.0.3"
  },
  "dependencies": {}
}
